{
  "loopback-component-explorer": {
    "mountPath": "/explorer",
    "generateOperationScopedModels": true
  },
  "@mean-expert/loopback-component-realtime": {
    "debug": false,
    "auth": true,
    "driver": {
      "name": "socket.io"
