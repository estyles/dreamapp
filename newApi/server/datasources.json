{
  "db_local": {
    "name": "db_local",
    "connector": "memory",
    "file": "mydata.json"
  },
  "db": {
    "name": "db",
    "connector": "mysql",
    "host": "${DB_HOST}",
    "port": "${DB_PORT}",
    "database": "${DB_NAME}",
    "password": "${DB_PASSWORD}",
    "user": "${DB_USER}",
    "debug": false,
    "collation": "utf8_general_ci"
  },
  "emailDs": {
    "name": "emailDs",
    "connector": "mail",
    "transports": [
      {
        "type": "smtp",
        "host": "${EDS_HOST}",
        "port": 465,
        "secure": true,
        "auth": {
          "user": "${EDS_USER}",
          "pass": "${EDS_PASSWORD}"
        }
      }
    ]
  }
}
