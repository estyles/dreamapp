  "Member": {
    "dataSource": "mariadb",
    "public": true
  },
  "Group": {
    "dataSource": "mariadb",
    "public": true
  },
  "AnswerList": {
    "dataSource": "mariadb",
    "public": true
  },
  "Answer": {
    "dataSource": "mariadb",
    "public": true
  },
  "Game": {
    "dataSource": "mariadb",
    "public": true
  },
  "Stage": {
    "dataSource": "mariadb",
    "public": true
  },
  "StageItem": {
    "dataSource": "mariadb",
    "public": true
  },
  "DataQuery": {
    "dataSource": "mariadb",
    "public": true
  }
